<div id="content-top">
  <h3>Plukkliste Deichman Hovedbiblioteket</h3>
  <table id="holdstable" class="tablesorter" >
    <thead>
      <tr>
        <th>hyllesign</th>
        <th>lok</th>
        <th>type</th>
        <th>Ã¥r</th>
        <th>res.dato</th>
        <th>avd</th>
        <th>forfatter</th>
        <th>tittel</th>
        <th>strekkode</th>
        <th hidden="true">bnr</th>
        <th>tnr</th>
        <th>lnr</th>
        <th>Hvem<br/>henter</th>
        <th>Funnet</th>
      </tr>
    </thead>
    <tbody>
      <% books.forEach(function(hold, index, array) { %>
        <tr id="row<%=index%>">
          <td><%= hold.itemcallnumber ? hold.itemcallnumber : '' %></td>
          <td><%= hold.location %></td>
          <td><%= hold.itype %></td>
          <td><%= hold.copyrightdate %></td>
          <td><%= hold.reservedate %></td>
          <td><%= hold.pickbranch %></td>
          <td><%= hold.author ? hold.author : '' %></td>
          <td><%= hold.title %></td>
          <td><%= hold.barcode %></td>
          <td class="biblionumber" hidden="true"><%= hold.biblionumber %></td>
          <td class="itemnumber"><%= hold.itemnumber %></td>
          <td class="borrowernumber"><%= hold.borrowernumber %></td>
          <td>Liv</td>
          <td>
            <% if (hold.found) { %> 
              <a href="#" class="found" id="star" ></a>
            <% } else { %>
              <a href="#" class="unfound" id="star" ></a>
            <% } %>
          </td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</div>
